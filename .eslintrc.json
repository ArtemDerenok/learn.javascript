{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
        "no-console": "off",
        "no-alert": "off",
        "no-restricted-globals": ["error", "event", "fdescribe"]
    }
}
